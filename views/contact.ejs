<%- include("partials/header"); -%>

	<h1 class="main-title">CONTACT</h1><br>
	<div class="row">
		<div class="col-lg-6 col-md-6 col-sm-12"
			<div class="container">
				<form action="/contact" method="post">
					<div class="form-group">
						<label class="main-para">Name</label>
						<input type="text" class="form-control inp" required>
					</div>
					<div class="form-group">
						<label class="main-para">Subject</label>
						<input type="text" class="form-control inp" required>
					</div>
					<div class="form-group">
						<label class="main-para">Email</label>
						<input type="email" class="form-control inp" required>
					</div>
					<div class="form-group">
						<label class="main-para">Website</label>
						<input type="text" class="form-control inp" required>
					</div>
					<div class="form-group">
						<label class="main-para">Message</label>
						<textarea class="form-control inp" rows="5" cols="5"></textarea>
					</div>
					<button class="btn main-para">SEND</button>
				</form>
			</div>
		</div>
	</div>
	
<%- include("partials/footer"); -%>